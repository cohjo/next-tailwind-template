  "dependencies": {
    "@next/font": "^13.2.4",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.11.11",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "autoprefixer": "^10.4.14",
    "concurrently": "^7.6.0",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "tailwindcss": "^3.2.7",
    "typescript": "4.9.4"
  }
